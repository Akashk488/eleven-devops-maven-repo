pipeline {
    agent any

    stages{

        stage('Build'){

            steps{

                echo " first stage is build"


            }
        }

       stage ('Unit-test'){

         steps{
          
          echo "Seconmd stage is Unit test"
         }

       }
        


       stage ('deploy'){

        steps{
            echo "third stage is  Deployment"
        }
       }
    }
}
